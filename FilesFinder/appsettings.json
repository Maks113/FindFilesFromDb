{
    "Serilog": {
        "Using":  [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
        "MinimumLevel": "Debug",
        "WriteTo": [
            { "Name": "Console" },
            { "Name": "File", "Args": { "path": "Logs/log.txt" } }
        ],
        "Properties": {
            "Application": "FilesFinder"
        }
    },
    "DatabaseConfig": {
        "ServerName": "",
        "User": "",
        "Password": "",
        "DatabaseName": ""
    },
    "FinderConfiguration": {
        "Targets": [
            {
                "TableName": "tbl_contract",
                "PathField": "[doc_path]",
                "IdField": "[ID]",
                "InfoFields": [
                    {
                        "FieldName": "[NUM_OF]",
                        "Description": "номер контракта"
                    }
                ]
            }
        ]
    }
}